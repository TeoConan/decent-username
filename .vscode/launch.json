{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run test",
            "type": "node",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/build/test.js",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
            "runtimeArgs": [],
            "env": {
                "NODE_ENV": "development"
            },
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/build/**/*.js"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}